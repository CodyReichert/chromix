{
   "name": "chromix",
   "version": "0.1.2",
   "author": "Stephen Blott <smblott@gmail.com>",
   "description": "Chromix is a command-line and scripting utility for controlling Google chrome. It can be used, amongst other things, to create, switch, focus, reload and remove tabs.",
   "homepage": [
      "https://github.com/smblott-github/chromix"
   ],
   "repository": {
      "type": "git",
      "url": "https://github.com/smblott-github/chromix"
   },
   "licenses" : [ {"type" : "MIT" } ],  
   "engines" : { "node" : ">=0.4" },
   "scripts": {
      "prepublish": "make snapshot"
   },
   "bin": {
      "chromix": "./bin/chromix",
      "chromix-server": "./bin/chromix-server"
   },
   "keywords": [ "chrome", "chromi", "extension", "cli", "command", "line" ],
   "dependencies": {
      "ws" :  "*",
      "optimist" :  "*"
   }
